<mvc:View controllerName="pro.proj.controller.work"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt">
    <Shell id="shell">
        <App id="app">

            <pages>

                <Page id="page" title="Project Details" showNavButton="true" navButtonPress="goBack" enableScrolling="true" class="sapUiResponsivePadding--header sapUiResponsivePadding--footer">
                     <headerContent>
                        <SearchField width="12rem" liveChange=".onSearch"/>
                    </headerContent>
                    <FlexBox height="100px" alignItems="Start" justifyContent="Center">
                        <Image src="model/proj.png" class="sapUiLargeMarginTop sapUiLargeMarginBottom" alt="Example picture of USB flash drive" />
                    </FlexBox>


                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>
