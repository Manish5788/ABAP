    xmlns="sap.m"
    xmlns:tnt="sap.tnt">
    <Shell id="shell">
        <App id="app">

            <pages>

                <Page id="page" title="Blog Details" showNavButton="true" navButtonPress="goBack" enableScrolling="true" class="sapUiResponsivePadding--header sapUiResponsivePadding--footer">
                    <FlexBox height="100px" alignItems="Start" justifyContent="Center">
                        <!-- <Carousel class="sapUiContentPadding" loop="true"> -->

                        <Image src="model/blog.jpg" class="sapUiLargeMarginTop sapUiLargeMarginBottom" alt="Example picture of USB flash drive" />

                        <!-- </Carousel> -->

                    </FlexBox>

                   
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>
